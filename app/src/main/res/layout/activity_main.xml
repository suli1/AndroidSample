<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.suli.sample.MainActivity"
    >

  <TextView
      android:id="@+id/tv_message"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Hello World!"
      />

  <Button
      android:id="@+id/btn_crash"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="crash test"
      />

  <Button
      android:id="@+id/btn_webview"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="go to webview"
      />

  <Button
      android:id="@+id/btn_encrypt"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:text="加解密压测"
      />

  <Button
      android:id="@+id/btn_goto_plugin_app"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Go to plugin app"
      />

  <Button
      android:id="@+id/btn_goto_plugin_demo1"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Go to plugin demo1"
      />

  <Button
      android:id="@+id/btn_goto_plugin_share"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:text="Go to plugin share"
      />
</LinearLayout>
